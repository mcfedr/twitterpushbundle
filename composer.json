{
    "name": "mcfedr/twitterpushbundle",
    "type": "symfony-bundle",
    "description": "A bundle for pushing tweets are push notifications",
    "keywords": ["push", "notification", "bundle", "gcm", "c2dm", "ios", "apns", "twitter"],
    "require": {
        "php": ">=5.4.0",
        "mcfedr/awspushbundle": "~4.0",
        "symfony/symfony": "~2.3",
        "guzzle/guzzle": "~3.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Mcfedr\\TwitterPushBundle": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Mcfedr\\TwitterPushBundle": "test/" }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    }
}
